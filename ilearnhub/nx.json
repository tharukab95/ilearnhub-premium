{
  "npmScope": "ilearnhub",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "core-auth": {
      "tags": ["tutor", "student"]
    },
    "core-data-access": {
      "tags": ["tutor", "student"]
    },
    "core-material": {
      "tags": ["tutor", "student"]
    },
    "core-ui": {
      "tags": ["tutor", "student"]
    },
    "student-portal": {
      "tags": ["student"]
    },
    "student-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["student-portal"]
    },
    "tutor-portal": {
      "tags": ["tutor"]
    },
    "tutor-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["tutor-portal"]
    }
  }
}
