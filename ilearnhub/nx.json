{
  "npmScope": "ilearnhub",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "admin-portal": {
      "tags": []
    },
    "admin-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["admin-portal"]
    },
    "auth": {
      "tags": []
    },
    "data-models": {
      "tags": []
    },
    "tutee-portal": {
      "tags": []
    },
    "tutee-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["tutee-portal"]
    },
    "tutor-portal": {
      "tags": []
    },
    "tutor-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["tutor-portal"]
    }
  }
}
