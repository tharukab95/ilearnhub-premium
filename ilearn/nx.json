{
  "npmScope": "ilearn",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "admin-portal": {
      "tags": []
    },
    "admin-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["admin-portal"]
    },
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "payment-portal": {
      "tags": []
    },
    "payment-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["payment-portal"]
    },
    "shared-auth": {
      "tags": ["shared"]
    },
    "tuition-portal": {
      "tags": []
    },
    "tuition-portal-e2e": {
      "tags": [],
      "implicitDependencies": ["tuition-portal"]
    }
  }
}
